{
  "name": "remind-me",
  "version": "1.3.0",
  "description": "A command-line reminder tool that supports natural language dates and times, inspired by Slack's remind feature",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "cron": "node ./lib/cron.js",
    "setup": "node ./lib/setup.js",
    "teardown": "node ./lib/teardown.js",
    "postinstall": "npm run setup",
    "postuninstall": "npm run teardown"
  },
  "bin": {
    "remind": "./index.js",
    "remind-me": "./index.js"
  },
  "preferGlobal": true,
  "repository": "github:zeke/remind-me",
  "keywords": [
    "calendar",
    "date",
    "time",
    "reminders",
    "gtd",
    "slack"
  ],
  "author": "zeke",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zeke/remind-me/issues"
  },
  "homepage": "https://github.com/zeke/remind-me#readme",
  "dependencies": {
    "chrono-node": "^1.0.6",
    "cp": "^0.2.0",
    "crontab": "^1.1.0",
    "dateparser": "^1.0.6",
    "easypattern": "0.0.2",
    "mkdirp": "^0.5.1",
    "node-notifier": "^4.2.3",
    "node-slack": "0.0.7",
    "prettydate": "0.0.1",
    "require-dir": "^0.3.0",
    "rimraf": "^2.4.3",
    "tmp": "0.0.27",
    "twilio": "^2.3.0",
    "user-home": "^2.0.0",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "mocha": "^2.2.5",
    "standard": "^5.1.1"
  }
}
